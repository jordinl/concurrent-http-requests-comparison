#!/usr/bin/env sh

rm -rf data/*
mkdir -p data
curl https://www.domcop.com/files/top/top10milliondomains.csv.zip -o /tmp/urls.csv.zip
7z e -so /tmp/urls.csv.zip > /tmp/urls.csv
cat /tmp/urls.csv | tail -n +2 | grep -v -e doubleclick | awk -F '","' '{print "https://"$2}' > data/urls.txt
rm /tmp/urls.csv*
